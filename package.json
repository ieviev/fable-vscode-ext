{
  "name": "verycoolextension",
  "displayName": "verycoolextension",
  "description": "",
  "version": "0.0.20",
  "publisher": "developer",
  "icon": "",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://localhost:1234"
  },
  "bugs": {
    "url": "http://localhost:1234"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:fsharp"
  ],
  "main": "./dist/main.js",
  "contributes": {
    "commands": [
      {
        "command": "newext.helloworld",
        "title": "newext: helloworld"
      }
    ]
  },
  "scripts": {
    "install": "dotnet tool restore",
    "build": "dotnet fable src -o build --run npm run esbuild",
    "start:dev": "dotnet fable watch src -s -o build --run npm run esbuild -- --watch",
    "esbuild": "esbuild ./build/Main.js --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node --sourcemap",
    "prepack": "npm run build",
    "patch": "vsce package patch --baseContentUrl https://localhost --baseImagesUrl https://localhost"
  },
  "dependencies": {
    "ionide-vscode-helpers": "ionide/ionide-vscode-helpers"
  },
  "devDependencies": {
    "esbuild": "^0.13.15",
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
